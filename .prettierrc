{
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "endOfLine": "crlf",
    "plugins": [
        "@trivago/prettier-plugin-sort-imports",
        "prettier-plugin-tailwindcss"
    ],
    "importOrder": [
        "use client",
        "^react$",
        "<THIRD_PARTY_MODULES>",
        "^context|^common/(.*)",
        "^components",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
